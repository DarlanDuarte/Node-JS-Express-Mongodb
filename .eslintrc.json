{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "eslint:recommended", "plugin:prettier/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "class-methods-use-this": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "linebreak-style": ["error", "unix"],
    "prettier/prettier": ["error", {}, { "endOfLine": "auto" }]
  }
}
